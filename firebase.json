{
  "hosting": {
    "site": "tokosaya",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/parfum",
        "destination": "/parfum/index.html"
      },
      {
        "source": "/perawatan",
        "destination": "/perawatan/index.html"
      },
      {
        "source": "/pakaian",
        "destination": "/pakaian/index.html"
      },
      {
        "source": "/tas",
        "destination": "/tas/index.html"
      },
      {
        "source": "/tugas",
        "destination": "/tugas/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
